<Window x:Class="BugTracker.NewProject"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="NewProject" Height="127" Width="376">
    <Grid>
        <Button Content="Cancel" HorizontalAlignment="Left" Margin="273,56,0,0" VerticalAlignment="Top" Width="75" Click="Cancel_Click"/>
        <Button Content="OK" HorizontalAlignment="Left" Margin="187,56,0,0" VerticalAlignment="Top" Width="75" Click="OK_Click"/>
        <Label Content="Enter the new project's name: " HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="187,13,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="161" Name="ProjectName"/>
    </Grid>
</Window>
