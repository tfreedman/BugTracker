<Window x:Class="BugTracker.SetUser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Set Active User" Height="166" Width="479">
    <Grid>
        <Label Content="Enter your username and password:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Button Content="Log In" HorizontalAlignment="Left" Margin="378,97,0,0" VerticalAlignment="Top" Width="75" Click="Login_Click"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="159,41,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="200" Name="UsernameBox"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="159,75,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="200" Name="PasswordBox"/>
        <Label Content="Username:" HorizontalAlignment="Left" Margin="88,39,0,0" VerticalAlignment="Top"/>
        <Label Content="Password:" HorizontalAlignment="Left" Margin="88,70,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
